{
  "expo": {
    "name": "FitLog Native RN",
    "slug": "fitlog-native-rn",
    "version": "0.0.1",
    "orientation": "portrait",
    "scheme": "fitlognative",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.fitlognativern"
    },
    "web": {
      "bundler": "metro"
    }
  }
}
